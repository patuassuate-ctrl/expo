{
  "name": "@expo/log-box",
  "version": "0.0.13",
  "main": "app/index.ts",
  "scripts": {
    "start": "expo",
    "clean": "run-p clean:lib clean:bundle",
    "clean:lib": "rimraf build",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:lib build:bundle",
    "build:lib": "tsc -p tsconfig.lib.json --watch",
    "build:bundle": "node ./scripts/watch-bundle.mjs",
    "prepare": "run-s clean prepare:lib prepare:bundle",
    "prepare:lib": "tsc -p tsconfig.lib.json",
    "prepare:bundle": "node ./scripts/build-bundle.mjs",
    "export:web": "expo export -p web --no-minify"
  },
  "dependencies": {},
  "devDependencies": {
    "@expo/spawn-async": "^1.7.2",
    "@types/react": "~19.1.0",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.0.1",
    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-web": "*"
  },
  "license": "MIT",
  "author": "Expo",
  "homepage": "https://github.com/expo/expo/tree/main/packages/@expo/log-box",
  "description": "Expo LogBox provides an improved debugging UI for React Native, succeeding the default RedBox and LogBox."
}
